@define normal{
    page{
        styles{
            .H1:{
                color: themeColors[4],
                background: themeColors[1],
                position : (50,10),
                origin : center,
                size : (100,20),
                font-size:50,
                font-bold
            }
        }

        @define header{
            group(
                    position : (50,10),
                    size     : (100,20),
                    origin : center,
            ){
                styles:{
                    "*" :{
                        font-size:50,
                        font-bold,
                    },
                    text :{
                        at:center
                        color:white
                        position:(50,50)
                    }
                }
                rectangle(
                    background: themeColors[1],
                    position:(0,0),
                    size:(100,100)
                    color: themeColors[1],
                    draw-contour:true,
                )
                rectangle(
                    background: themeColors[2],
                    position:(0,98),
                    size:(100,3)
                )
                polygon(
                    5, show,
                    color: themeColors[2],
                    size:(14,70),
                    at:center
                    position:(10,50),
                    rotate:0,
                    stroke:basic(width:3)
                )
                polygon(
                    5, show,
                    color: themeColors[3],
                    size:(14,70),
                    at:center
                    position:(10,50),
                    rotate: 20
                    stroke:basic(width:3)
                )
                polygon(
                    5, show,
                    color: themeColors[4],
                    size:(14,70),
                    at:center
                    position:(10,50),
                    rotate: 40
                    stroke:basic(width:3)
                ),
                image("avatars/vpc.png", origin:center, position:(90,50),size:(10,40),preserve-aspect-ratio)
                if(themeShowPalette){
                    Palette()
                }
                componentBody
            }
        }

